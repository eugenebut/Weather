{
  "name": "Weather",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Shows weather in current location. Powered by Yahoo! Weather.",
  "browser_action": {
    "default_icon": "loading.png"
  },
  "options_page": "options.html",
  "permissions": [
    "http://weather.yahooapis.com/",
    "http://l.yimg.com/",
    "geolocation"
  ],
  "background": {
    "persistent": true,
    "page": "background.html"
  }
}
